	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) imcan",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/imcan",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "(gdb) dbcan_test",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/dbcan/dbcan_test",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/dbc",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	]
}